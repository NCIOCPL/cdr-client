<?xml version="1.0"?>
<!DOCTYPE Citation SYSTEM "Citation.dtd">
<Citation xmlns:cdr="cips.nci.nih.gov/cdr"> 
 <CdrDocCtl> 
  <DocId><?xm-replace_text {
         The document ID will be assigned automatically
         }?></DocId>
  <DocTitle><?xm-replace_text {
         The document title will be constructed automatically
         }?></DocTitle>
 </CdrDocCtl> 
 <VerificationDetails>
  <Verified><?xm-replace_text {
         Select Yes or No (required)
         }?></Verified> 
  <VerifiedIn><?xm-replace_text {
         Optionally select a verification source
         }?></VerifiedIn>
 </VerificationDetails> 
 <PDQCitation> 
  <CitationType><?xm-replace_text {
         Select a citation type (required)
         }?></CitationType>
  <CitationTitle><?xm-replace_text {
         Enter the title (required)
         }?></CitationTitle>
  <AuthorList Complete="Yes"> 
   <Author> 
    <LastName><?xm-replace_text {
         Enter the author's last name (required)
         }?></LastName>
    <FirstName><?xm-replace_text {
         Enter the author's initials (required)
         }?></FirstName>
   </Author> 
   <Author> 
    <CollectiveName><?xm-replace_text {
         Enter a collective name author, or Change to a personal 
         name author (required)
         }?></CollectiveName>
   </Author> 
  </AuthorList> 
  <PublicationDetails> 
   <PublishedIn><?xm-replace_text {
         Optionally link to a parent citation
         }?></PublishedIn>
   <OtherPublicationInformation>
    <PublicationInformation><?xm-replace_text {
         Enter volume, issue, and pagination for an article; 
         place, publisher, edition, and year for a book (required)
         }?></PublicationInformation>
    <InternetInformation><?xm-replace_text {
         Optionally enter available Internet information
         }?></InternetInformation>
   </OtherPublicationInformation>
   <PublicationYear><?xm-replace_text {
         Optionally enter the citation's year of publication (for sorting)
         }?></PublicationYear>
  </PublicationDetails> 
 </PDQCitation> 
</Citation>
