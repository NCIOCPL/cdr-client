<?xml version="1.0"?>
<!DOCTYPE PDQBoardMemberInfo SYSTEM "PDQBoardMemberInfo.dtd">
<PDQBoardMemberInfo xmlns:cdr="cips.nci.nih.gov/cdr">
 <CdrDocCtl>
  <DocId><?xm-replace_text {
